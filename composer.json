{
    "name": "sanmai/version-info",
    "type": "library",
    "description": "Version Info Parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexey Kopytko",
            "email": "alexey@kopytko.com",
            "homepage": "https://www.alexeykopytko.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.8",
        "friendsofphp/php-cs-fixer": "^2.16",
        "infection/infection": ">=0.16",
        "phan/phan": ">1.3.0",
        "php-coveralls/php-coveralls": "^2.3",
        "phpstan/phpstan": ">=0.10",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": ">=3.0.16"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "VersionInfo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\VersionInfo\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/sanmai/version-info/issues"
    }
}
